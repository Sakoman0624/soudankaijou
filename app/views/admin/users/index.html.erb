<h1>全ユーザー一覧</h1>
  <table class="table table-striped">
    <thead>
      <tr>
        <th>
          ユーザー名
        </th>
        <th></th>
      </tr>
    </thead>
    <tbody>
    <% @users.each do |user| %>
    <tr>
        <td><%= user.name %> - <%= user.email %></td>
        <td><%= link_to '削除', admin_user_path(user), method: :delete, data: { confirm: '本当に削除しますか？' } %></td>
    </tr>
    <% end %>
    </tbody>
  </table>